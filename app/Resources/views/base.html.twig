{% set nocostumizerstyle = true %}
{% set compress = true %}
{% set form_layout = 'MetronicAdminBundle::form/empty.html.twig' %}
{% extends "MetronicAdminBundle::base.html.twig" %}
{% block title %}World Sticker Price{% endblock title %}
{% block basecss %}
{% if compress is defined and compress %}
    {% stylesheets 
        '@MetronicAdminBundle/Resources/public/assetic/plugins/font-awesome/css/font-awesome.min.css'
        '@MetronicAdminBundle/Resources/public/assetic/plugins/bootstrap/css/bootstrap.min.css'
        '@MetronicAdminBundle/Resources/public/assetic/plugins/uniform/css/uniform.default.css'

        '@MetronicAdminBundle/Resources/public/assetic/css/style-metronic.css'
        '@MetronicAdminBundle/Resources/public/assetic/css/style.css'
        '@MetronicAdminBundle/Resources/public/css/style-responsive.css'
        '@MetronicAdminBundle/Resources/public/assetic/css/plugins.css'
        '@MetronicAdminBundle/Resources/public/css/custom.css'

        '@EphpUtilityBundle/Resources/public/css/utility.css'
        '@WSPAdminBundle/Resources/public/css/custom.css'
     %}
        <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
    {% endstylesheets %}
    <link href="{{ asset('/bundles/metronicadmin/css/themes/default.css') }}" rel="stylesheet" type="text/css" id="style_color"/>
{% else %}
    {% include "MetronicAdminBundle::layout/stylesheets.html.twig" %}
    <link href="/bundles/ephputility/css/utility.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/wspadmin/css/custom.css" rel="stylesheet" type="text/css"/>
{% endif %}
{% endblock %}